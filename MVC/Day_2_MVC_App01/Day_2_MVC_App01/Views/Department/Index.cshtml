@model List<Department>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>


<a href="/Department/Add" class="btn btn-success">New</a>

<table class="table table-bordered table-hover">
    <tr>
        <th>Id</th>
        <th>Name</th>
    </tr>

@foreach (var item in Model){
    <tr>
        <td>@item.Id</td>
        <td>@item.Name</td>
    </tr>
}
</table>
