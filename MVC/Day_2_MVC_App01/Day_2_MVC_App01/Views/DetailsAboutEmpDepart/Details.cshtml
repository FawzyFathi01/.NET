@model DetailsDepartmentEmployee
@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<table class="table table-bordered table-hover">
    <tr>
        <th>EmpID</th>
        <th>EmpName</th>
        <th>Salary</th>
        <th>Age</th>
        <th>DepartName</th>
    </tr>
    <tr>
        <td>@Model.EmployeeID</td>
        <td>@Model.EmployeeName</td>
        <td>@Model.Salary</td>
        <td>@Model.EmployeeAge</td>
        <td>@Model.DepartName</td>
    </tr>
</table>
